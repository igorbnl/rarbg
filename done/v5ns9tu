function ShowHide(id,id1) {
	$("#"+id+"").toggle();
	$("#"+id1+"").toggle();

	if($("#"+id+"").is(":visible")) {
		$('#showhidefiles').html('Hide Files &laquo;');
	} else {
		$('#showhidefiles').html('Show Files &raquo;');
	}
}

function p_a_d(url,p,m,mn,pre,sh) {
	var p_l = '';var p_r = '';
	if(pre) { p_l = '<pre>';p_r = '</pre>'; }
	var p_e_c = $("#"+p);
	var m_e_c = $("#"+m);
	if(p_e_c.html() == "---" || !sh) {
		p_e_c.empty().html('<img src="https://dyncdn.me/static/20/img/loading.gif" border="0" />');
		$.get(url, function(d) { p_e_c.html(p_l+d+p_r); });
	} else {
		p_e_c.toggle();
	}

	if(sh) {
		if(p_e_c.is(":visible")) {
			m_e_c.html(mn.replace('Show','Hide')+" &laquo;");
		} else {
			m_e_c.html(mn+" &raquo;");
		}
	}

}

